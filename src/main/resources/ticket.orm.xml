<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name = "com.andersen.lesson9.Models.Ticket" table = "public.ticket">
        <id name = "id" type = "int" column = "id">
            <generator class="increment">
                <param name="sequence">ticket_seq</param>
            </generator>
        </id>
        <many-to-one name="user" column="user_id"
                     class="com.andersen.lesson9.Models.User" />
        <property name="ticketType" column="ticket_type">
            <type name="org.hibernate.type.EnumType">
                <param name="enumClass">com.andersen.lesson9.Models.TicketType</param>
                <param name="type">12</param>
            </type>
        </property>
        <property name = "creationDate" column = "creation_date" type = "timestamp"/>
    </class>
</hibernate-mapping>